    </owl:ObjectProperty>
    


    <!-- https://w3id.org/cao#hasBaseReading -->

    <owl:ObjectProperty rdf:about="https://w3id.org/cao#hasBaseReading">
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#CriticalApparatusEntryContent"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#ReadingInApparatusCause"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/cao#hasReading -->

    <owl:ObjectProperty rdf:about="https://w3id.org/cao#hasReading">
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#CriticalApparatusEntryContent"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#ReadingInApparatus"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/cao#hasReadingCause -->

    <owl:ObjectProperty rdf:about="https://w3id.org/cao#hasReadingCause">
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#ReadingInApparatus"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#ReadingInApparatusCause"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/cao#hasReadingType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/cao#hasReadingType">
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#ReadingInApparatus"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#ReadingInApparatusType"/>
    </owl:ObjectProperty>
    


