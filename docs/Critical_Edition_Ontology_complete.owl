    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/critical-edition-ontology#AccompanyingMaterial -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#AccompanyingMaterial">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#ScholarlyEditionComponent"/>
        <rdfs:comment xml:lang="en">Component of a scholarly edition of a textual or other nature that accompanies the edited text and is subordinate to it.</rdfs:comment>
        <rdfs:label xml:lang="en">Accompanying Material</rdfs:label>
        <rdfs:comment xml:lang="it">Componente di un’edizione scientifica di natura testuale o di altro tipo che accompagna il testo edito e ad esso è subordinato.</rdfs:comment>
        <rdfs:label xml:lang="it">Materiale di corredo</rdfs:label>
    </owl:Class>
    


    <!-- https://www.w3.org/ns/oa#Annotation -->

    <owl:Class rdf:about="https://www.w3.org/ns/oa#Annotation">
        <owl:equivalentClass rdf:resource="http://purl.org/critical-edition-ontology#ApparatusEntry"/>
        <rdfs:comment xml:lang="en">The class for Web Annotations.</rdfs:comment>
        <rdfs:label xml:lang="en">Annotation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#Apparatus -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#Apparatus">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#AccompanyingMaterial"/>
        <rdfs:comment xml:lang="en">Component of a scholarly edition that accompanies the edited text and consists of a set of annotations that the editor has appended to edited text for a particular scientific purpose.</rdfs:comment>
        <rdfs:label xml:lang="en">Apparatus</rdfs:label>
        <rdfs:comment xml:lang="it">Componente di un’edizione scientifica che accompagna un testo edito ed è costituito da un insieme di annotazioni che l’editore ha apposto al testo edito per un particolare obiettivo scientifico.</rdfs:comment>
        <rdfs:label xml:lang="it">Apparato</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#ApparatusEntry -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#ApparatusEntry">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#AccompanyingMaterial"/>
        <rdfs:comment xml:lang="en">An annotation presented within an apparatus.</rdfs:comment>
        <rdfs:label xml:lang="en">Apparatus Entry</rdfs:label>
        <rdfs:comment xml:lang="it">Un’annotazione presentata all’interno di un apparato.</rdfs:comment>
        <rdfs:label xml:lang="it">Voce d’apparato</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#ApparatusEntryContent -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#ApparatusEntryContent">
        <owl:equivalentClass rdf:resource="http://www.w3.org/ns/oa#TextualBody"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#AccompanyingMaterial"/>
        <rdfs:comment xml:lang="en">Textual content of an apparatus entry.</rdfs:comment>
        <rdfs:label xml:lang="en">Apparatus Entry Content</rdfs:label>
        <rdfs:comment xml:lang="it">Contenuto testuale di una voce d’apparato.</rdfs:comment>
        <rdfs:label xml:lang="it">Contenuto della voce d'apparato</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#BaseReadingInApparatus -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#BaseReadingInApparatus">
        <owl:equivalentClass rdf:resource="https://w3id.org/cao#BaseReading"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#ReadingInApparatus"/>
        <rdfs:comment xml:lang="en">The reading chosen or proposed by the editor, presented within an entry of the critical apparatus for the dual purpose of indicating the passage in the text to which the entry refers and to allow the reader to compare it more easily with the other recorded readings.</rdfs:comment>
        <rdfs:label xml:lang="en">Base Reading In Apparatus</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#BaseWitness -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#BaseWitness">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#Witness"/>
        <rdfs:comment xml:lang="en">Witness used as a basis to establish the critical text.</rdfs:comment>
        <rdfs:label xml:lang="en">Base Witness</rdfs:label>
        <rdfs:comment xml:lang="it">Testimone adottato come base per l’allestimento del testo critico.</rdfs:comment>
        <rdfs:label xml:lang="it">Testimone di base</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#Commentary -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#Commentary">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#Apparatus"/>
        <rdfs:comment xml:lang="en">A type of apparatus that the editor appends to the critical text in order to offer more information on the textual transmission and to argue their editorial choices.</rdfs:comment>
        <rdfs:label xml:lang="en">Commentary</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#CriticalApparatus -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#CriticalApparatus">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#Apparatus"/>
        <rdfs:comment xml:lang="en">An apparatus through which the editor shows the differences between the critical text and the textual transmission and possibly explains their choices.</rdfs:comment>
        <rdfs:label xml:lang="en">Critical Apparatus</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#CriticalApparatusEntry -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#CriticalApparatusEntry">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#ApparatusEntry"/>
        <rdfs:comment xml:lang="en">An annotation to the critical text that is part of the critical apparatus, within which the editor presents the variants and, more generally, the readings attested in the textual tradition, comparing them with the relative passage in the critical text.</rdfs:comment>
        <rdfs:label xml:lang="en">Critical Apparatus Entry</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#CriticalApparatusEntryContent -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#CriticalApparatusEntryContent">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#ApparatusEntryContent"/>
        <rdfs:comment xml:lang="en">The content of a critical apparatus entry.</rdfs:comment>
        <rdfs:label xml:lang="en">Critical Apparatus Entry Content</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#CriticalApparatusEntryNote -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#CriticalApparatusEntryNote">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#CriticalApparatusEntryContent"/>
        <rdfs:comment xml:lang="en">Note that the editor inserts in an entry of the critical apparatus to specify some philological aspects of a reading, of a group of readings, of the chosen or proposed reading or, in general, of the textual tradition at that point of the text.</rdfs:comment>
        <rdfs:label xml:lang="en">Critical Apparatus Entry Note</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#CriticalApparatusGroup -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#CriticalApparatusGroup">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#AccompanyingMaterial"/>
        <rdfs:comment xml:lang="en">Structure in which the critical apparatus can be divided, in order to distinguish the different types of information it contains and facilitate its reading. A critical apparatus group may contain a particular type of readings (for example substantive variants or orthographic variants) or it may group together the information about the variation in a given set of witnesses (for example a family of witnesses).</rdfs:comment>
        <rdfs:label xml:lang="en">Critical Apparatus Group</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#CriticalEdition -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#CriticalEdition">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#ScholarlyEdition"/>
        <rdfs:comment xml:lang="en">A type of scholarly edition, created by applying the principles of textual criticism, with the aim of reconstructing the original form or more original forms of the text of a work.</rdfs:comment>
        <rdfs:label xml:lang="en">Critical Edition</rdfs:label>
        <rdfs:comment xml:lang="it">Un tipo di edizione scientifica, realizzata applicando i principi della critica testuale, allo scopo di ricostruire la forma originaria o le 
            forme originarie del testo di un’opera.
        </rdfs:comment>
        <rdfs:label xml:lang="it">Edizione critica</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#CriticalIntervention -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#CriticalIntervention">
        <rdfs:subClassOf rdf:resource="http://purl.org/emmedi/hico#InterpretationAct"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/critical-edition-ontology#isCarriedOutOn"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://purl.org/critical-edition-ontology#Reading"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">During the preparation of the critical text, it is the interpretative act through which the editor establishes or reconstructs the portion of text to be included in the critical text and the readings to be presented in the critical apparatus, starting from the readings present in the textual transmission.</rdfs:comment>
        <rdfs:label xml:lang="en">Critical Intervention</rdfs:label>
        <rdfs:comment xml:lang="it">In fase di allestimento del testo critico, è l’atto interpretativo mediante il quale l’editore stabilisce o ricostruisce la porzione di testo da accogliere nel testo critico e le lezioni da presentare nell’apparato critico, partendo dalle lezioni presenti nella tradizione testuale.</rdfs:comment>
        <rdfs:label xml:lang="it">Intervento critico</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#CriticalInterventionCriterion -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#CriticalInterventionCriterion">
        <rdfs:subClassOf rdf:resource="http://purl.org/emmedi/hico#InterpretationCriterion"/>
        <rdfs:comment xml:lang="en">The criterion or more broadly the reason why the editor has deemed a critical intervention necessary.</rdfs:comment>
        <rdfs:label xml:lang="en">Critical Intervention Criterion</rdfs:label>
        <rdfs:comment xml:lang="it">Il criterio o più in generale la motivazione che ad avviso dell’editore hanno reso necessario l'intervento critico.</rdfs:comment>
        <rdfs:label xml:lang="it">Criterio dell'intervento critico</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#CriticalInterventionType -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#CriticalInterventionType">
        <rdfs:subClassOf rdf:resource="http://purl.org/emmedi/hico#InterpretationType"/>
        <rdfs:comment xml:lang="en">The type of critical intervention made by the editor, for example: the correction of an evident error, the selection of a variant (selectio), the conjecture (divinatio), etc.</rdfs:comment>
        <rdfs:label xml:lang="en">Critical Intervention Type</rdfs:label>
        <rdfs:comment xml:lang="it">La tipologia dell’intervento critico effettuato dall’editore, per esempio: la correzione di un errore evidente, la scelta di una variante (selectio), la congettura (divinatio), ecc.</rdfs:comment>
        <rdfs:label xml:lang="it">Tipo di intervento critico</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#CriticalText -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#CriticalText">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#EditedText"/>
        <rdfs:comment xml:lang="en">A type of edited text presented in a critical edition and edited for the purpose of reconstructing the original form or one of the original forms of the text of a work.</rdfs:comment>
        <rdfs:label xml:lang="en">Critical Text</rdfs:label>
        <rdfs:comment xml:lang="it">Un tipo di testo edito presentato all’interno di un’edizione critica e allestito allo scopo di ricostruire la forma originaria o una delle forme originarie del testo di un’opera.</rdfs:comment>
        <rdfs:label xml:lang="it">Testo critico</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#CriticalTextPassage -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#CriticalTextPassage">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#EditedTextPassage"/>
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E90_Symbolic_Object"/>
        <rdfs:comment xml:lang="en">A portion of variable size of the critical text, made up of characters, words or sentences.</rdfs:comment>
        <rdfs:label xml:lang="en">Critical Text Passage</rdfs:label>
        <rdfs:comment xml:lang="it">Una porzione di dimensione variabile del testo critico, costituita da caratteri, parole o frasi.</rdfs:comment>
        <rdfs:label xml:lang="it">Luogo del testo critico</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#EditedText -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#EditedText">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#ScholarlyEditionComponent"/>
        <rdfs:comment xml:lang="en">Text of a work prepared by an editor or a group of editors within a scholarly edition.</rdfs:comment>
        <rdfs:label xml:lang="en">Edited Text</rdfs:label>
        <rdfs:comment xml:lang="it">Testo di un’opera allestito da un editore o da un gruppo di editori presentato all’interno di un’edizione scientifica.</rdfs:comment>
        <rdfs:label xml:lang="it">Testo edito</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#EditedTextPassage -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#EditedTextPassage">
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E90_Symbolic_Object"/>
        <rdfs:comment xml:lang="en">A portion of variable size of the edited text, made up of characters, words or sentences.</rdfs:comment>
        <rdfs:label xml:lang="en">Edited Text Passage</rdfs:label>
        <rdfs:comment xml:lang="it">Una porzione di dimensione variabile del testo edito, costituita da caratteri, parole o frasi.</rdfs:comment>
        <rdfs:label xml:lang="it">Passo del testo edito</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#Editor -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#Editor">
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E39_Actor"/>
        <rdfs:comment xml:lang="en">Person that created a scholarly edition. The editor must possess philological skills and textual criticism skills. The identity of the editor must be known, in order to make their work citable.</rdfs:comment>
        <rdfs:label xml:lang="en">Editor</rdfs:label>
        <rdfs:comment xml:lang="it">Persona che realizza un’edizione scientifica. L’editore deve possedere competenze di filologia e di critica testuale. L’identità dell'editore deve essere nota, per rendere il suo lavoro citabile.</rdfs:comment>
        <rdfs:label xml:lang="it">Editore</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#EditorsGroup -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#EditorsGroup">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#Editor"/>
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E74_Group"/>
        <rdfs:comment xml:lang="en">A group of editors that collaboratively create a scholarly edition.</rdfs:comment>
        <rdfs:label xml:lang="en">Editors Group</rdfs:label>
        <rdfs:comment xml:lang="it">Un gruppo di editori che ha realizzato collaborativamente un’edizione scientifica.</rdfs:comment>
        <rdfs:label xml:lang="it">Gruppo di editori</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#ExternalCriticalInterventionCriterion -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#ExternalCriticalInterventionCriterion">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#CriticalInterventionCriterion"/>
        <rdfs:comment xml:lang="en">A criterion for carrying out the critical intervention based on the genealogy of the textual transmission.</rdfs:comment>
        <rdfs:label xml:lang="en">External Critical Intervention Criterion</rdfs:label>
        <rdfs:comment xml:lang="it">Un criterio per effettuare l’intervento critico basato sulla genealogia della tradizione testuale.</rdfs:comment>
        <rdfs:label xml:lang="it">Criterio esterno dell'intervento critico</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#F1_Work -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#F1_Work">
        <rdfs:comment xml:lang="en">This class comprises distinct intellectual ideas conveyed in artistic and intellectual creations, such a poems, stories or musical compositions.</rdfs:comment>
        <rdfs:label xml:lang="en">F1 Work</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#F2_Expression -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#F2_Expression">
        <rdfs:comment xml:lang="en">This class comprises the intellectual or artistic realisations of Works in the form of identifiable immaterial objects, such as texts, poems, jokes, musical or choreographic notations, movement pattern, sound pattern, images, multimedia objects, or any combination of such forms. The substance of F2 Expression is signs.</rdfs:comment>
        <rdfs:label xml:lang="en">F2 Expression</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#F3_Manifestation -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#F3_Manifestation">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:comment xml:lang="en">This class comprises products rendering one or more Expressions. A Manifestation is defined by both the overall content and the form of its presentation. The substance of F3 Manifestation is not only signs, but also the manner in which they are presented to be consumed by users, including the kind of media adopted.</rdfs:comment>
        <rdfs:label xml:lang="en">F3 Manifestation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#Gap -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#Gap">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#Reading"/>
        <rdfs:comment xml:lang="en">Missing passage within a witness. It may be due to a voluntary or involuntary omission on the part of the copyist, a mechanical gap, an erasure or the state of conservation of the witness.</rdfs:comment>
        <rdfs:label xml:lang="en">Gap</rdfs:label>
        <rdfs:comment xml:lang="it">Passo mancante all’interno di un testimone. Può essere dovuto a un’omissione volontaria o involontaria da parte del copista, a una lacuna meccanica, a una cancellatura o allo stato di conservazione del testimone.</rdfs:comment>
        <rdfs:label xml:lang="it">Passo mancante</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#InternalCriticalInterventionCriterion -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#InternalCriticalInterventionCriterion">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#CriticalInterventionCriterion"/>
        <rdfs:comment xml:lang="en">A criterion for carrying out the critical intervention based on the intrinsic plausibility of the variants.</rdfs:comment>
        <rdfs:label xml:lang="en">Internal Critical Intervention Criterion</rdfs:label>
        <rdfs:comment xml:lang="it">Un criterio per effettuare l’intervento critico basato sulla plausibilità intrinseca delle varianti.</rdfs:comment>
        <rdfs:label xml:lang="it">Criterio interno dell'intervento critico</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#Introduction -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#Introduction">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#AccompanyingMaterial"/>
        <rdfs:comment xml:lang="en">Text of variable size which introduces the edited text within a scholarly edition, presenting the work itself and the work carried out by the editor. The introduction may be organized in different parts and may contain lists, pictures, and other materials.</rdfs:comment>
        <rdfs:label xml:lang="en">Introduction</rdfs:label>
        <rdfs:comment xml:lang="it">Testo di dimensione variabile che all’interno di un’edizione scientifica introduce il testo edito, presentando l’opera e il lavoro svolto dall’editore. L’introduzione può essere articolata in più parti e può contenere elenchi, immagini e altri tipi di materiali.</rdfs:comment>
        <rdfs:label xml:lang="it">Introduzione</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#IntroductionPart -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#IntroductionPart">
        <rdfs:comment xml:lang="en">A part of the introduction.</rdfs:comment>
        <rdfs:label xml:lang="en">Introduction Part</rdfs:label>
        <rdfs:comment xml:lang="it">Una parte dell’introduzione.</rdfs:comment>
        <rdfs:label xml:lang="it">Parte dell’introduzione</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#Note -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#Note">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#ApparatusEntry"/>
        <rdfs:comment xml:lang="en">An annotation to the critical text, which is part of the commentary.</rdfs:comment>
        <rdfs:label xml:lang="en">Note</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#NoteContent -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#NoteContent">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#ApparatusEntryContent"/>
        <rdfs:comment xml:lang="en">Content of a note.</rdfs:comment>
        <rdfs:label xml:lang="en">Note Content</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#PassagePositionSelector -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#PassagePositionSelector">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#PassageSelector"/>
        <rdfs:comment xml:lang="en">Type of selector of a passage of the critical text which indicates where the passage is positioned, usually through the number of the structural element in which the passage is contained (for example the verse, the line, the paragraph, etc.) or through the cross reference to an alphanumeric exponent or other graphic sign.</rdfs:comment>
        <rdfs:label xml:lang="en">Passage Position Selector</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#PassageQuoteSelector -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#PassageQuoteSelector">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#PassageSelector"/>
        <rdfs:comment xml:lang="en">Type of selector of a passage of the critical text which indicates the passage by copying it in its entirety or in abbreviated form.</rdfs:comment>
        <rdfs:label xml:lang="en">Passage Quote Selector</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#PassageSelector -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#PassageSelector">
        <owl:equivalentClass rdf:resource="http://www.w3.org/ns/oa#Selector"/>
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E41_Appellation"/>
        <rdfs:comment xml:lang="en">Sequence of characters that allows to identify exact-ly a passage within a text. There are several types of selector. Often it is a reference to the location of the passage, for example the line or verse number, or a copy of the passage concerned.</rdfs:comment>
        <rdfs:label xml:lang="en">Passage Selector</rdfs:label>
        <rdfs:comment xml:lang="it">Sequenza di caratteri che permette di individuare esattamente un passo all’interno di un testo. Ci sono diversi tipi di selettore. Spesso si tratta di un riferimento alla posizione del passo, per esempio il numero di riga o verso, o di una copia del passo interessato.</rdfs:comment>
        <rdfs:label xml:lang="it">Selettore del passo</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#Reader -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#Reader">
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E21_Person"/>
        <rdfs:comment xml:lang="en">Person that reads the scholarly edition.
            Note: the term “reads” was chosen to sum up the different ways in which a critical edition can be used.
        </rdfs:comment>
        <rdfs:label xml:lang="en">Reader</rdfs:label>
        <rdfs:comment xml:lang="it">Persona che legge l’edizione scientifica.
            Nota: con un generico “legge” si riassumono le diverse modalità in cui un’edizione critica può essere fruita.
        </rdfs:comment>
        <rdfs:label xml:lang="it">Lettore</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#Reading -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#Reading">
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E90_Symbolic_Object"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/critical-edition-ontology#isReadingOf"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://purl.org/critical-edition-ontology#Witness"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A sequence of characters, words, or phrases that can be read in a preserved or lost witness.</rdfs:comment>
        <rdfs:label xml:lang="en">Reading</rdfs:label>
        <rdfs:comment xml:lang="it">Una sequenza di caratteri, parole o frasi che può essere letta in un testimone conservato o andato perduto.</rdfs:comment>
        <rdfs:label xml:lang="it">Lezione</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#ReadingGroup -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#ReadingGroup">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#CriticalApparatusEntryContent"/>
        <rdfs:comment xml:lang="en">A set of readings within an entry of the critical apparatus, grouped to highlight some peculiarities of the textual tradition or to facilitate consultation. A group of readings is graphically distinct from the other contents of the critical apparatus entry.</rdfs:comment>
        <rdfs:label xml:lang="en">Reading Group</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#ReadingInApparatus -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#ReadingInApparatus">
        <owl:equivalentClass rdf:resource="https://w3id.org/cao#Reading"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#CriticalApparatusEntryContent"/>
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E90_Symbolic_Object"/>
        <rdfs:comment xml:lang="en">Reading presented in a critical apparatus entry. A reading in the critical apparatus may correspond to two or more readings of different witnesses. The reading in the critical apparatus can be presented in a diplomatic (according to a particular witness) or interpretative form.</rdfs:comment>
        <rdfs:label xml:lang="en">Reading In Apparatus</rdfs:label>
        <rdfs:comment xml:lang="it">Lezione presentata in una voce dell’apparato critico. Una lezione nell’apparato critico può corrispondere in realtà a due o più lezioni di testimoni diversi. La lezione nell’apparato critico può essere riportata in veste diplomatica (secondo la lettera di un particolare testimone) o in veste interpretativa.</rdfs:comment>
        <rdfs:label xml:lang="it">Lezione in apparato</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#ReadingInApparatusCause -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#ReadingInApparatusCause">
        <owl:equivalentClass rdf:resource="https://w3id.org/cao#ReadingCause"/>
        <rdfs:comment xml:lang="en">The type of error made by the copyist during the act of copying or the factor that may have led the copyist to produce an erroneous or innovative reading.</rdfs:comment>
        <rdfs:label xml:lang="en">Reading In Apparatus Cause</rdfs:label>
        <rdfs:comment xml:lang="it">Il tipo di errore commesso dal copista durante l’operazione di copia o il fattore che può aver indotto il copista a produrre una lezione erronea o innovativa.</rdfs:comment>
        <rdfs:label xml:lang="it">Causa della lezione in apparato</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#ReadingInApparatusType -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#ReadingInApparatusType">
        <owl:equivalentClass rdf:resource="https://w3id.org/cao#ReadingType"/>
        <rdfs:comment xml:lang="en">Category that the editor assigns to a lesson to classify it and make their evaluation explicit.</rdfs:comment>
        <rdfs:label xml:lang="en">Reading In Apparatus Type</rdfs:label>
        <rdfs:comment xml:lang="it">Categoria che l’editore attribuisce a una lezione per classificarla ed esplicitare la propria valutazione.</rdfs:comment>
        <rdfs:label xml:lang="it">Tipo della lezione in apparato</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#ScholarlyEdition -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#ScholarlyEdition">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:comment xml:lang="en">The content of a publication that makes the text of a literary work or work of historical-cultural interest, mostly of the past, otherwise inaccessible, accessible to the public. The edition is called “scholarly” as it is created by a scholar or a group of scholars, applying shared methodologies in their own philological field.</rdfs:comment>
        <rdfs:label xml:lang="en">Scholarly Edition</rdfs:label>
        <rdfs:comment xml:lang="it">Il contenuto di una pubblicazione che rende accessibile al pubblico il testo di un’opera letteraria o di interesse storico-culturale, perlopiù del passato, altrimenti inaccessibile. L’edizione è detta “scientifica” in quanto è realizzata da uno studioso o da un gruppo di studiosi, applicando delle metodologie condivise nel proprio ambito filologico.</rdfs:comment>
        <rdfs:label xml:lang="it">Edizione scientifica</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#ScholarlyEditionComponent -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#ScholarlyEditionComponent">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:comment xml:lang="en">A component of a scholarly edition. Usually the component of a scholarly edition is a text, but it can also be an image, a multimedia object or another kind of object. Among the main components of a scholarly edition are the edited text and the materials accompanying the edited text.</rdfs:comment>
        <rdfs:label xml:lang="en">Scholarly Edition Component</rdfs:label>
        <rdfs:comment xml:lang="it">Un componente di un’edizione scientifica. Solitamente il componente di un’edizione scientifica è un testo, ma può essere anche un’immagine, un oggetto multimediale o un altro tipo di oggetto. Tra i principali componenti di un’edizione scientifica ci sono il testo edito e i materiali di corredo al testo edito.</rdfs:comment>
        <rdfs:label xml:lang="it">Componente di un’edizione scientifica</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#ScholarlyEditionPublication -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#ScholarlyEditionPublication">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#F3_Manifestation"/>
        <rdfs:comment xml:lang="en">The publication of a scholarly edition, which comprises both the content (the scholarly edition itself), its presentation and the material support.</rdfs:comment>
        <rdfs:label xml:lang="en">Scholarly Edition Publication</rdfs:label>
        <rdfs:comment xml:lang="it">La pubblicazione dell’edizione scientifica, che comprende non solo il suo contenuto (l’edizione scientifica in sé) ma anche il modo in cui si presenta e il suo supporto fisico.</rdfs:comment>
        <rdfs:label xml:lang="it">Pubblicazione dell'edizione scientifica</rdfs:label>
    </owl:Class>
    


    <!-- https://www.w3.org/ns/oa#Selector -->

    <owl:Class rdf:about="https://www.w3.org/ns/oa#Selector">
        <rdfs:comment xml:lang="en">A resource which describes the segment of interest in a representation of a Source resource, indicated with oa:hasSelector from the Specific Resource. This class is not used directly in the Annotation model, only its subclasses.</rdfs:comment>
        <rdfs:label xml:lang="en">Selector</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#Siglum -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#Siglum">
        <rdfs:comment xml:lang="en">Alphanumeric string that identifies a witness within a critical edition.</rdfs:comment>
        <rdfs:label xml:lang="en">Siglum</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#SiglumReference -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#SiglumReference">
        <rdfs:comment xml:lang="en">Reference to a witness siglum, used within the critical apparatus (and in other apparatuses), next to readings and optionally to reconstructed passage references.</rdfs:comment>
        <rdfs:label xml:lang="en">Siglum Reference</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#StemmaCodicum -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#StemmaCodicum">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#AccompanyingMaterial"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#IntroductionPart"/>
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E36_Visual_Item"/>
        <rdfs:comment xml:lang="en">Graph that represents the genealogical tree of the textual transmission of a work.</rdfs:comment>
        <rdfs:label xml:lang="en">Stemma Codicum</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#SurfaceWitness -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#SurfaceWitness">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#Witness"/>
        <rdfs:comment xml:lang="en">Witness used as a reference to establish the linguistic style of the critical text.</rdfs:comment>
        <rdfs:label xml:lang="en">Surface Witness</rdfs:label>
        <rdfs:comment xml:lang="it">Testimone adottato come riferimento per stabilire la forma linguistica del testo critico.</rdfs:comment>
        <rdfs:label xml:lang="it">Testimone di superficie</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/oa#TextualBody -->

    <owl:Class rdf:about="http://www.w3.org/ns/oa#TextualBody"/>
    
    


    <!-- http://purl.org/critical-edition-ontology#TextualTradition -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#TextualTradition">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/critical-edition-ontology#hasPart"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://purl.org/critical-edition-ontology#Witness"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Set of witnesses that preserve a work.</rdfs:comment>
        <rdfs:label xml:lang="en">Textual Tradition</rdfs:label>
        <rdfs:comment xml:lang="it">Insieme dei testimoni che tramandano un’opera.</rdfs:comment>
        <rdfs:label xml:lang="it">Tradizione testuale</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#UnclearPassage -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#UnclearPassage">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#Reading"/>
        <rdfs:comment xml:lang="en">Witness passage of variable size, which the editor cannot read and report with certainty due to how it appears within its material support.</rdfs:comment>
        <rdfs:label xml:lang="en">Unclear Passage</rdfs:label>
        <rdfs:comment xml:lang="it">Passo del testimone di dimensione variabile, che l’editore non può leggere e riportare con certezza per via di come si presenta all’interno del proprio supporto materiale.</rdfs:comment>
        <rdfs:label xml:lang="it">Luogo incerto del testo</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#Witness -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#Witness">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/critical-edition-ontology#isPartOf"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://purl.org/critical-edition-ontology#TextualTradition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/critical-edition-ontology#isPreservedIn"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://purl.org/critical-edition-ontology#WitnessCarrier"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Copy of the text of a work.</rdfs:comment>
        <rdfs:label xml:lang="en">Witness</rdfs:label>
        <rdfs:comment xml:lang="it">Copia del testo di un’opera.</rdfs:comment>
        <rdfs:label xml:lang="it">Testimone</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#WitnessCarrier -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#WitnessCarrier">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#F3_Manifestation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/critical-edition-ontology#preserves"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://purl.org/critical-edition-ontology#Witness"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The physical support of a witness, usually a manuscript or a printed book.</rdfs:comment>
        <rdfs:label xml:lang="en">Witness Carrier</rdfs:label>
        <rdfs:comment xml:lang="it">Il supporto materiale di un testimone, solitamente un manoscritto o un libro a stampa.</rdfs:comment>
        <rdfs:label xml:lang="it">Supporto del testimone</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#WitnessDescription -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#WitnessDescription">
        <rdfs:comment xml:lang="en">Text that can be provided within the witnesses list, to describe a witness in more or less depth and dealing with different aspects: the state of conservation, the material aspect, the palaeographic analysis, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">Witness Description</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/critical-edition-ontology#WitnessesList -->

    <owl:Class rdf:about="http://purl.org/critical-edition-ontology#WitnessesList">
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#AccompanyingMaterial"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/critical-edition-ontology#IntroductionPart"/>
        <rdfs:comment xml:lang="en">List of the witnesses that preserve the text of a work. The list is presented within a critical edition, usually inside the introduction.</rdfs:comment>
        <rdfs:label xml:lang="en">Witnesses List</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:Class>
    



    


    <!-- http://purl.org/emmedi/hico#InterpretationAct -->

    <owl:Class rdf:about="http://purl.org/emmedi/hico#InterpretationAct"/>
    


    <!-- http://purl.org/emmedi/hico#InterpretationCriterion -->

    <owl:Class rdf:about="http://purl.org/emmedi/hico#InterpretationCriterion"/>
    


    <!-- http://purl.org/emmedi/hico#InterpretationType -->

    <owl:Class rdf:about="http://purl.org/emmedi/hico#InterpretationType"/>
    


