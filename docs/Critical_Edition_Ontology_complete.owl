    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/oa#hasBody">
        <rdfs:label xml:lang="en">has body</rdfs:label>
        <rdfs:comment xml:lang="en">The object of the relationship is a resource that is a body of the Annotation.</rdfs:comment>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:ObjectProperty>
    


