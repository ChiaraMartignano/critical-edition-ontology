    <!-- http://purl.org/critical-edition-ontology#R15_has_fragment -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#R15_has_fragment">
        <owl:inverseOf rdf:resource="http://purl.org/critical-edition-ontology#R15_is_fragment_of"/>
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:range rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E90_Symbolic_Object"/>
        <rdfs:comment xml:lang="en">This property associates an instance of E90 Symbolic Object with an instance of F2 Expression of which it is a fragment. The fragment is not itself an instance of F2 Expression as it does not express any instance of F1 Work. When the fragment consists of intelligible words it is an instance of E33 Linguistic Object.</rdfs:comment>
        <rdfs:label xml:lang="en">R15 has fragment</rdfs:label>
        <rdfs:seeAlso>https://www.cidoc-crm.org/frbroo/sites/default/files/LRMoo_V0.9%28draft%20for%20WLIC%202022%29.pdf</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#R15_is_fragment_of -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#R15_is_fragment_of">
        <rdfs:domain rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E90_Symbolic_Object"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:label xml:lang="en">R15 is fragment of</rdfs:label>
        <rdfs:seeAlso>https://www.cidoc-crm.org/frbroo/sites/default/files/LRMoo_V0.9%28draft%20for%20WLIC%202022%29.pdf</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#R3_is_realised_in -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#R3_is_realised_in">
        <owl:inverseOf rdf:resource="http://purl.org/critical-edition-ontology#R3_realises"/>
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#F1_Work"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:label xml:lang="en">R3 is realised in</rdfs:label>
        <rdfs:seeAlso>https://www.cidoc-crm.org/frbroo/sites/default/files/LRMoo_V0.9%28draft%20for%20WLIC%202022%29.pdf</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#R3_realises -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#R3_realises">
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#F1_Work"/>
        <rdfs:comment xml:lang="en">This property associates an instance of F2 Expression with an instance of F1 Work.</rdfs:comment>
        <rdfs:label xml:lang="en">R3 realises</rdfs:label>
        <rdfs:seeAlso>https://www.cidoc-crm.org/frbroo/sites/default/files/LRMoo_V0.9%28draft%20for%20WLIC%202022%29.pdf</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#R4_embodies -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#R4_embodies">
        <owl:inverseOf rdf:resource="http://purl.org/critical-edition-ontology#R4_is_embodied_in"/>
        <owl:inverseOf rdf:resource="http://purl.org/critical-edition-ontology#R5_is_component_of"/>
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#F3_Manifestation"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:comment xml:lang="en">This property associates an instance of F3 Manifestation with one or more instances of F2 Expression which are rendered by this instance of F3 Manifestation. The manifestation formats the expression(s) in the way they are to be presented to some public, including specifying the intended sensory impression (such as visual appearance or audio rendition).</rdfs:comment>
        <rdfs:label xml:lang="en">R4 embodies</rdfs:label>
        <rdfs:seeAlso>https://www.cidoc-crm.org/frbroo/sites/default/files/LRMoo_V0.9%28draft%20for%20WLIC%202022%29.pdf</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#R4_is_embodied_in -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#R4_is_embodied_in">
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#F3_Manifestation"/>
        <rdfs:label xml:lang="en">R4 is embodied in</rdfs:label>
        <rdfs:seeAlso>https://www.cidoc-crm.org/frbroo/sites/default/files/LRMoo_V0.9%28draft%20for%20WLIC%202022%29.pdf</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#R5_has_component -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#R5_has_component">
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:comment xml:lang="en">This property associates an instance of an F2 Expression X with a structural component Y that conveys a part of the overall work realized by X, such as volumes, chapters, or sections. This property is transitive, asymmetric and irreflexive.</rdfs:comment>
        <rdfs:label xml:lang="en">R5 has component</rdfs:label>
        <rdfs:seeAlso>https://www.cidoc-crm.org/frbroo/sites/default/files/LRMoo_V0.9%28draft%20for%20WLIC%202022%29.pdf</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#R5_is_component_of -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#R5_is_component_of">
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:label xml:lang="en">R5 is component of</rdfs:label>
        <rdfs:seeAlso>https://www.cidoc-crm.org/frbroo/sites/default/files/LRMoo_V0.9%28draft%20for%20WLIC%202022%29.pdf</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#R76_has_derivative -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#R76_has_derivative">
        <owl:inverseOf rdf:resource="http://purl.org/critical-edition-ontology#R76_is_derivative_of"/>
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:comment xml:lang="en">This property associates an instance of F2 Expression with another instance of F2 Expression which was its source or one of its sources. This property is not transitive. It is asymmetric and irreflexive.</rdfs:comment>
        <rdfs:label xml:lang="en">R76 has derivative</rdfs:label>
        <rdfs:seeAlso>https://www.cidoc-crm.org/frbroo/sites/default/files/LRMoo_V0.9%28draft%20for%20WLIC%202022%29.pdf</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#R76_is_derivative_of -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#R76_is_derivative_of">
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:label xml:lang="en">R76 is derivative of</rdfs:label>
        <rdfs:seeAlso>https://www.cidoc-crm.org/frbroo/sites/default/files/LRMoo_V0.9%28draft%20for%20WLIC%202022%29.pdf</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#accompanies -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#accompanies">
        <owl:inverseOf rdf:resource="http://purl.org/critical-edition-ontology#isAccompaniedBy"/>
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#AccompanyingMaterial"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#EditedText"/>
        <rdfs:comment xml:lang="en">This property associates an instance of Accompanying Material with the instance of Edited Text it accompanies. An edited text can have multiple accompanying materials.</rdfs:comment>
        <rdfs:label xml:lang="en">accompanies</rdfs:label>
        <rdfs:comment xml:lang="it">Questa proprietà associa un’istanza di Accompanying Material all’istanza di Edited Text che correda. Un testo edito può avere molteplici materiali di corredo.</rdfs:comment>
        <rdfs:label xml:lang="it">correda</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#apparatusEntryHasContent -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#apparatusEntryHasContent">
        <owl:inverseOf rdf:resource="http://purl.org/critical-edition-ontology#isContentOfApparatusEntry"/>
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#ApparatusEntry"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#ApparatusEntryContent"/>
        <rdfs:comment xml:lang="en">This property associates an apparatus entry, i.e. an instance of Apparatus Entry, with its content, an instance of Apparatus Entry Content.</rdfs:comment>
        <rdfs:label xml:lang="en">apparatus entry has content</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#apparatusEntryRefersTo -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#apparatusEntryRefersTo">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P67_refers_to"/>
        <owl:inverseOf rdf:resource="http://purl.org/critical-edition-ontology#isReferredToByApparatusEntry"/>
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#ApparatusEntry"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#EditedTextPassage"/>
        <rdfs:comment xml:lang="en">This property associates an Apparatus Entry instance with the Edited Text Passage instance it refers to.</rdfs:comment>
        <rdfs:label xml:lang="en">apparatus entry refers to</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#commentaryHasNote -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#commentaryHasNote">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/critical-edition-ontology#hasEntry"/>
        <owl:inverseOf rdf:resource="http://purl.org/critical-edition-ontology#isNoteOfCommentary"/>
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#Commentary"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#Note"/>
        <rdfs:comment xml:lang="en">This property associates a philological comment, instance of Commentary, with a note it contains, instance of Note. There is at least one note within a commentary.</rdfs:comment>
        <rdfs:label xml:lang="en">commentary has note</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#created -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#created">
        <owl:inverseOf rdf:resource="http://purl.org/critical-edition-ontology#wasCreatedBy"/>
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#Editor"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#ScholarlyEdition"/>
        <rdfs:comment xml:lang="en">This property associates an instance of Editor or EditorsGroup with an instance of Scholarly Edition they created.</rdfs:comment>
        <rdfs:label xml:lang="en">created</rdfs:label>
        <rdfs:comment xml:lang="it">Questa proprietà associa un’istanza di Editor o di EditorsGroup a un’istanza di Scholarly Edition da loro creata.</rdfs:comment>
        <rdfs:label xml:lang="it">ha creato</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#criticalApparatusEntryHasContent -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#criticalApparatusEntryHasContent">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/critical-edition-ontology#apparatusEntryHasContent"/>
        <owl:inverseOf rdf:resource="http://purl.org/critical-edition-ontology#isContentOfCriticalApparatusEntry"/>
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#CriticalApparatusEntry"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#CriticalApparatusEntryContent"/>
        <rdfs:comment xml:lang="en">This property associates an instance of Critical Apparatus Entry with its own content, instance of Critical Apparatus Entry Content. Within the same critical apparatus entry there are contents of a different nature, represented by instances of the Critical Apparatus Entry Content subclasses.</rdfs:comment>
        <rdfs:label xml:lang="en">critical apparatus entry has content</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#criticalApparatusEntryRefersTo -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#criticalApparatusEntryRefersTo">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/critical-edition-ontology#apparatusEntryRefersTo"/>
        <owl:inverseOf rdf:resource="http://purl.org/critical-edition-ontology#isReferredToByCriticalApparatusEntry"/>
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#CriticalApparatusEntry"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#CriticalTextPassage"/>
        <rdfs:comment xml:lang="en">This property associates an entry of the critical apparatus, an instance of Critical Apparatus Entry, with the passage of the critical text to which it refers, an instance of Critical Text Passage.</rdfs:comment>
        <rdfs:label xml:lang="en">critical apparatus entry refers to</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#criticalApparatusHasEntry -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#criticalApparatusHasEntry">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/critical-edition-ontology#hasEntry"/>
        <owl:inverseOf rdf:resource="http://purl.org/critical-edition-ontology#isEntryOfCriticalApparatus"/>
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#CriticalApparatus"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#CriticalApparatusEntry"/>
        <rdfs:comment xml:lang="en">This property associates an instance of Critical Apparatus with an instance of Critical Apparatus Entry. A critical apparatus contains at least one entry.</rdfs:comment>
        <rdfs:label xml:lang="en">critical apparatus has entry</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#criticalApparatusHasGroup -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#criticalApparatusHasGroup">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/critical-edition-ontology#R5_has_component"/>
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#CriticalApparatus"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#CriticalApparatusGroup"/>
        <rdfs:comment xml:lang="en">This property associates an instance of Critical Apparatus, with the groups into which it can be subdivided, that are instances of Critical Apparatus Group.</rdfs:comment>
        <rdfs:label xml:lang="en">critical apparatus has group</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#editionHasComponent -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#editionHasComponent">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/critical-edition-ontology#R5_has_component"/>
        <owl:inverseOf rdf:resource="http://purl.org/critical-edition-ontology#isComponentOfEdition"/>
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#ScholarlyEdition"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#ScholarlyEditionComponent"/>
        <rdfs:comment xml:lang="en">This property associates an instance of Scholarly Edition with an instance of Scholarly Edition Component.</rdfs:comment>
        <rdfs:label xml:lang="en">edition has component</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#entryContentHasNote -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#entryContentHasNote">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P3_has_note"/>
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#CriticalApparatusEntryContent"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#CriticalApparatusEntryNote"/>
        <rdfs:comment xml:lang="en">This property associates an instance of Critical Apparatus Entry Note with an instance of Critical Apparatus Entry Content or one of its subclasses.</rdfs:comment>
        <rdfs:label xml:lang="en">entry content has note</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#entryHasReading -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#entryHasReading">
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#CriticalApparatusEntry"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#ReadingInApparatus"/>
        <rdfs:comment xml:lang="en">This property associates an instance of Critical Apparatus Entry with one or more instances of Reading In Apparatus. At least one reading is presented in each entry of the critical apparatus.</rdfs:comment>
        <rdfs:label xml:lang="en">entry has reading</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#follows -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#follows">
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#ReadingInApparatus"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#ReadingInApparatus"/>
        <rdfs:label xml:lang="en">follows</rdfs:label>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#hasApparatus -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#hasApparatus">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/critical-edition-ontology#isAccompaniedBy"/>
        <owl:inverseOf rdf:resource="http://purl.org/critical-edition-ontology#isApparatusOf"/>
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#EditedText"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#Apparatus"/>
        <rdfs:comment xml:lang="en">This property associates an Edited Text instance with an Apparatus instance. Multiple apparatuses can be associated with the same edited text.</rdfs:comment>
        <rdfs:label xml:lang="en">has apparatus</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#hasEntry -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#hasEntry">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/critical-edition-ontology#R5_has_component"/>
        <owl:inverseOf rdf:resource="http://purl.org/critical-edition-ontology#isEntryOf"/>
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#Apparatus"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#ApparatusEntry"/>
        <rdfs:comment xml:lang="en">This property associates an instance of Apparatus instance with an instance of Apparatus Entry. An apparatus contains at least one apparatus entry. Several apparatus entries are linked to an apparatus.</rdfs:comment>
        <rdfs:label xml:lang="en">has entry</rdfs:label>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it"></rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/critical-edition-ontology#hasFormalVariant -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#hasFormalVariant">
