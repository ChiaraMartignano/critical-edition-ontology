    </owl:DatatypeProperty>
    


    <!-- http://purl.org/critical-edition-ontology#isPositive -->

    <owl:DatatypeProperty rdf:about="http://purl.org/critical-edition-ontology#isPositive">
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#CriticalApparatus"/>
