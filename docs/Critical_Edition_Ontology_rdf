    <owl:Class rdf:about="https://w3id.org/cao#BaseReading"/>
    


    <!-- https://w3id.org/cao#Reading -->

    <owl:Class rdf:about="https://w3id.org/cao#Reading"/>
    


    <!-- https://w3id.org/cao#ReadingCause -->

    <owl:Class rdf:about="https://w3id.org/cao#ReadingCause"/>
    


    <!-- https://w3id.org/cao#ReadingType -->

    <owl:Class rdf:about="https://w3id.org/cao#ReadingType"/>
    


    <!-- 
