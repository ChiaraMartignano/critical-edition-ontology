    <!-- http://purl.org/critical-edition-ontology#R4_embodies -->

    <owl:ObjectProperty rdf:about="http://purl.org/critical-edition-ontology#R4_embodies">
        <owl:inverseOf rdf:resource="http://purl.org/critical-edition-ontology#R4_is_embodied_in"/>
        <owl:inverseOf rdf:resource="http://purl.org/critical-edition-ontology#R5_is_component_of"/>
        <rdfs:domain rdf:resource="http://purl.org/critical-edition-ontology#F3_Manifestation"/>
        <rdfs:range rdf:resource="http://purl.org/critical-edition-ontology#F2_Expression"/>
        <rdfs:comment xml:lang="en">This property associates an instance of F3 Manifestation with one or more instances of F2 Expression which are rendered by this instance of F3 Manifestation. The manifestation formats the expression(s) in the way they are to be presented to some public, including specifying the intended sensory impression (such as visual appearance or audio rendition).</rdfs:comment>
        <rdfs:label xml:lang="en">R4 embodies</rdfs:label>
